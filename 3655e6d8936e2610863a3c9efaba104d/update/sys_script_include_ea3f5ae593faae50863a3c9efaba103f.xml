    <sys_script_include action="INSERT_OR_UPDATE">
        <access>package_private</access>
        <active>true</active>
        <api_name>global.RescheduleAppointment</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>Reschedule appointment of patient</description>
        <mobile_callable>false</mobile_callable>
        <name>RescheduleAppointment</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var RescheduleAppointment = Class.create();
RescheduleAppointment.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	rescheduleAppointment: function() {
        var sysId = this.getParameter('sysparm_sys_id');
        var newDate = this.getParameter('sysparm_new_date');

        var gr = new GlideRecord('u_appointment');
        if (gr.get(sysId)) {
            gr.u_appointment_date = newDate;
            gr.update();

            gs.eventQueue('appointment.rescheduled', gr, gr.patient, newDate);

            return 'success';
        }

        return 'fail';
    },

    type: 'RescheduleAppointment'
});]]></script>
