        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[var AppointmentCancelled = Class.create();
AppointmentCancelled.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {

	cancelappointment:function()
	{
		var appointmentId = this.getParameter("sysparm_appoiid");
		var gr = new GlideRecord("u_appointment");
		gr.addQuery("sys_id", appointmentId);
		gr.query();
		if(gr.next() &&  gr.u_status!='cancelled')
		{
			
			gr.u_status = 'cancelled';
			gr.update();

			gs.eventQueue("appointment.cancelled",gr,null,null);
			return true;
		}
		return false;

		


	},

    type: 'AppointmentCancelled'
});]]></script>
